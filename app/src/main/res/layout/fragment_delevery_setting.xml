<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.DeleverySettingFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#f1f1f1"
            android:layout_marginBottom="@dimen/_20sdp"
            android:orientation="vertical">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/delivery_motorbike" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        android:layout_marginEnd="@dimen/_20sdp"
                        android:fontFamily="@font/raleway_semi_bold"
                        android:textColor="@color/txt_orderid"
                        android:textSize="@dimen/_14sdp"
                        android:text="Tell us the miles within which you are delivering the food:" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <EditText
                            android:id="@+id/distance"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:ems="10"
                            android:maxLength="1"
                            android:fontFamily="@font/open_sans_bold"
                            android:textSize="@dimen/_12sdp"
                            android:paddingEnd="@dimen/_5sdp"
                            android:paddingRight="@dimen/_5sdp"
                            android:paddingStart="@dimen/_5sdp"
                            android:paddingLeft="@dimen/_5sdp"
                            android:textColor="@color/bg_login_start"
                            android:gravity="center"
                            android:background="@drawable/border_six"
                            android:inputType="number"
                            android:textCursorDrawable="@drawable/cursor_color"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:textColor="@color/txt_orderid"
                            android:fontFamily="@font/open_sans_regular"
                            android:textSize="@dimen/_12sdp"
                            android:text="Miles (1-6)" />

                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_10sdp"
                android:orientation="horizontal">
                <ImageView
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/clock_circular_outline" />



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:layout_marginRight="@dimen/_20sdp"
                        android:layout_marginEnd="@dimen/_20sdp"
                        android:fontFamily="@font/raleway_semi_bold"
                        android:textColor="@color/txt_orderid"
                        android:textSize="@dimen/_14sdp"
                        android:text="Average Delivery Time:" />
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginStart="@dimen/_20sdp"
                        android:layout_marginLeft="@dimen/_20sdp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">
                        <EditText
                            android:id="@+id/delevery_time"
                            android:layout_width="@dimen/_30sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:ems="10"
                            android:maxLength="2"
                            android:textColor="@color/bg_login_start"
                            android:gravity="center"
                            android:paddingEnd="@dimen/_5sdp"
                            android:paddingRight="@dimen/_5sdp"
                            android:paddingStart="@dimen/_5sdp"
                            android:paddingLeft="@dimen/_5sdp"
                            android:textSize="@dimen/_12sdp"
                            android:fontFamily="@font/open_sans_bold"
                            android:background="@drawable/border_six"
                            android:inputType="number"
                            android:textCursorDrawable="@drawable/cursor_color"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"
                            android:textColor="@color/txt_orderid"
                            android:fontFamily="@font/open_sans_regular"
                            android:textSize="@dimen/_12sdp"
                            android:text="Minutes" />

                    </LinearLayout>
                </LinearLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_20sdp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Delivery Charge"
                        android:fontFamily="@font/raleway_semi_bold"
                        android:textColor="@color/txt_orderid"
                        android:textSize="@dimen/_10sdp"/>

                    <EditText
                        android:id="@+id/edtDeliveryCharge"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:fontFamily="@font/open_sans_bold"
                        android:textSize="@dimen/_12sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:padding="5dp"
                        android:textColor="@color/bg_login_start"
                        android:gravity="center"
                        android:background="@drawable/border_six"
                        android:inputType="number"
                        android:textCursorDrawable="@drawable/cursor_color"/>


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_20sdp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Minimum order value"

                        android:fontFamily="@font/raleway_semi_bold"
                        android:textColor="@color/txt_orderid"
                        android:textSize="@dimen/_10sdp"/>

                    <EditText
                        android:id="@+id/edtMinOrder"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:fontFamily="@font/open_sans_bold"
                        android:textSize="@dimen/_12sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:padding="5dp"
                        android:textColor="@color/bg_login_start"
                        android:gravity="center"
                        android:background="@drawable/border_six"
                        android:inputType="number"
                        android:textCursorDrawable="@drawable/cursor_color"/>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_20sdp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Free Delivery"
                        android:fontFamily="@font/raleway_semi_bold"
                        android:textColor="@color/txt_orderid"
                        android:textSize="@dimen/_10sdp"/>

                    <EditText
                        android:id="@+id/edtFreeDelivery"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:fontFamily="@font/open_sans_bold"
                        android:textSize="@dimen/_12sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingLeft="@dimen/_10sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:padding="5dp"
                        android:textColor="@color/bg_login_start"
                        android:gravity="center"
                        android:background="@drawable/border_six"
                        android:inputType="number"
                        android:textCursorDrawable="@drawable/cursor_color"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:layout_marginRight="@dimen/_20sdp">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Avg Preparation Time"
                        android:fontFamily="@font/raleway_semi_bold"
                        android:textColor="@color/txt_orderid"
                        android:textSize="@dimen/_10sdp"/>

                    <EditText
                        android:id="@+id/edtAvgTime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:maxLength="2"
                        android:fontFamily="@font/open_sans_bold"
                        android:textSize="@dimen/_12sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:paddingRight="@dimen/_10sdp"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingLeft="@dimen/_10sdp"
                        android:padding="5dp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:textColor="@color/bg_login_start"
                        android:gravity="center"
                        android:background="@drawable/border_six"
                        android:inputType="number"
                        android:textCursorDrawable="@drawable/cursor_color"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/liUpdateBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_100sdp"
                android:layout_marginRight="@dimen/_100sdp"
                android:gravity="center"
                android:background="@drawable/border_round"
               >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="15.00dp"
                    android:text="UPDATE"
                    android:padding="8dp"

                    />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_10sdp"
                android:background="#f1f1f1"
                android:orientation="vertical">
            </LinearLayout>

        </LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/bg_login_start"
            android:textSize="@dimen/_16sdp"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginEnd="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_20sdp"
            android:fontFamily="@font/raleway_bold"
            android:text="Following post codes are in 30 miles of radius for deliveries." />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginStart="@dimen/_20sdp"
            android:layout_marginLeft="@dimen/_20sdp"
            android:orientation="horizontal">


            <CheckBox
                android:id="@+id/chechbox_set_for_all"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:text=""
                android:buttonTint="@color/bg_login_center"
             />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginStart="@dimen/_10sdp"
                android:textColor="@color/txt_orderid"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/raleway_semi_bold"
                android:text="Set one amount for all post codes" />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp"
            android:layout_marginTop="@dimen/_10sdp"
            android:background="#707070"
            android:orientation="horizontal">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/_10sdp"
                android:layout_weight="1"
                android:background="#989898"
                android:layout_marginRight="@dimen/_1sdp"
                android:layout_marginEnd="@dimen/_1sdp"
                android:gravity="center"
                android:fontFamily="@font/raleway_semi_bold"
                android:textColor="@color/white"
                android:text="Post\nCode" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/_10sdp"
                android:textColor="@color/white"
                android:layout_weight="1"
                android:background="#989898"
                android:layout_marginRight="@dimen/_1sdp"
                android:layout_marginEnd="@dimen/_1sdp"
                android:gravity="center"
                android:fontFamily="@font/raleway_semi_bold"
                android:text="Min. Order\nValue" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/_10sdp"
                android:textColor="@color/white"
                android:layout_weight="1"
                android:background="#989898"
                android:layout_marginRight="@dimen/_1sdp"
                android:layout_marginEnd="@dimen/_1sdp"
                android:gravity="center"
                android:fontFamily="@font/raleway_semi_bold"
                android:text="Delivery\nCharges" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/_10sdp"
                android:layout_weight="1"
                android:layout_marginRight="@dimen/_1sdp"
                android:layout_marginEnd="@dimen/_1sdp"
                android:gravity="center"
                android:background="#989898"
                android:textColor="@color/white"
                android:fontFamily="@font/raleway_semi_bold"
                android:text="Free\nDelivery" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/_10sdp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#989898"
                android:textColor="@color/white"
                android:fontFamily="@font/raleway_semi_bold"
                android:text="Action" />
        </LinearLayout>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/recycler_time_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </LinearLayout>
    </ScrollView>
</FrameLayout>